    <entry>
      <title>使用jQuery制作游戏角色</title>
      <link href="/posts/473862c4.html"/>
      <url>/posts/473862c4.html</url>
      
        <content type="html"><![CDATA[<p>&lt;% draw %&gt;</p><meta charset="UTF-8"><style>    #character {        width: 100px;        height: 80px;        background-image: url('../img/siteicon/player1_big.gif'); /* 游戏角色素材 */        background-size: cover;        position: absolute;    }    #treasure {        width: 30px;        height: 30px;        background-color: gold;        position: absolute;        top: 100px;        left: 100px;    }</style><body><div id="character"></div><div id="treasure"></div><script src="https://code.jquery.com/jquery-3.6.0.min.js"></script><script>    $(document).ready(function() {        var score = 0;        $(document).keydown(function(e) {            var $character = $('#character');            var position = $character.position();            switch(e.key) {                case 'a': // left                    $character.css({left: position.left - 10 + 'px'});                    break;                case 'w': // up                    $character.css({top: position.top - 10 + 'px'});                    break;                case 'd': // right                    $character.css({left: position.left + 10 + 'px'});                    break;                case 's': // down                    $character.css({top: position.top + 10 + 'px'});                    break;            }        });        $('#character').dblclick(function() {            // 切换游戏角色            if ($(this).css('background-image').includes('player1_big')) {                $(this).css('background-image', 'url(../img/siteicon/player2.gif)'); //切换为2            } else {                $(this).css('background-image', 'url(../img/siteicon/player1_big.gif)'); //切换回1            }        });        setInterval(function() {            var $character = $('#character');            var $treasure = $('#treasure');            var characterPosition = $character.position();            var treasurePosition = $treasure.position();            if(Math.abs(characterPosition.left - treasurePosition.left) < 50 && Math.abs(characterPosition.top - treasurePosition.top) < 50) {                $treasure.css({top: Math.random() * $(window).height(), left: Math.random() * $(window).width()});                score++;                console.log('Score: ' + score);            }        }, 100);});</script></body><% endraw % >]]></content>
      
      
      
    </entry>
    
    
    
    <entry>
      <title>使用jQuery修改表格</title>
      <link href="/posts/753ecb61.html"/>
      <url>/posts/753ecb61.html</url>
      
        <content type="html"><![CDATA[<style type="text/css">        body {            font-size: 13px;            line-height: 25px;        }        table {            border-top: 1px solid #333;            border-left: 1px solid #333;            width: 300px;        }        td {            border-right: 1px solid #333;            border-bottom: 1px solid #333;        }        .center {            text-align: center;        }    </style><script src="https://code.jquery.com/jquery-3.3.1.min.js"></script><script>    //219971133向子豪    $(document).ready(function() {        $("#b1").click(function() {            var newRow = '<tr><td>幸福从天而降</td><td align="center">&yen;18.50</td></tr>';            $("#row3").before(newRow);        });        $("#b2").click(function() {            $("#row2").remove();        });        $("#b3").click(function() {            $("#row1").css({                'font-weight': 'bold',                'text-align': 'center',                'background-color': '#cccccc'            });        });        $("#b4").click(function() {            var oldRow = $("#row3").clone();            $("#myTable").append(oldRow);        });    });</script>    <body><table border="0" cellspacing="0" cellpadding="0" id="myTable"><tr id="row1">    <td>书名</td>    <td>价格</td></tr><tr id="row2">    <td>看得见风景的房间</td>    <td class="center">&yen;30.00</td></tr><tr id="row3">    <td>60个瞬间</td>    <td class="center">&yen;32.00</td></tr></table><input id="b1" type="button" value="增加一行" /><input id="b2" type="button" value="删除第2行" /><input id="b3" type="button" value="修改标题样式" /><input id="b4" type="button" value="复制最后一行" /></body>]]></content>
      
      
      
